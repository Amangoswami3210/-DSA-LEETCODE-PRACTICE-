class Solution
{
    public:
    //Function to count nodes of a linked list.
    int getCount(struct Node* head){
              Node* temp= head;
              int count=0;
              
              if(head==NULL){
                  return 0;
              }
        //Code here
        while(head!=NULL){
         count++;
         head=head->next;
        }
        
    return count;
    }
};
